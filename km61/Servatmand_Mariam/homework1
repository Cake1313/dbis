  CREATE TABLE "FURRR"."CAR" 
   (	"CAR_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"CAR_MODEL" VARCHAR2(15 BYTE) NOT NULL ENABLE, 
	"CAR_TYPE" VARCHAR2(15 BYTE) NOT NULL ENABLE, 
	"RELEASE_YEAR" NUMBER(4,0) NOT NULL ENABLE, 
	 CONSTRAINT "CAR_PK" PRIMARY KEY ("CAR_ID");
   
     CREATE TABLE "FURRR"."CITIZEN" 
   (	"CITIZEN_NAME" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	"PHONE_NUMBER" VARCHAR2(13 BYTE), 
	"PERSON_ID" NUMBER(12,0) NOT NULL ENABLE, 
	"AGE" NUMBER(3,0), 
	 CONSTRAINT "CITIZEN_PK" PRIMARY KEY ("PERSON_ID");
   
   CREATE TABLE "FURRR"."HOUSE" 
   (	"STREET" VARCHAR2(15 BYTE) NOT NULL ENABLE, 
	"FLOOR_NUMBER" NUMBER(3,0) NOT NULL ENABLE, 
	"H_ID" NUMBER(4,0) NOT NULL ENABLE, 
	 CONSTRAINT "HOUSE_PK" PRIMARY KEY ("H_ID");
   
     CREATE TABLE "FURRR"."CITIZEN_HOUSE" 
   (	"PERSON_ID" NUMBER(12,0) NOT NULL ENABLE, 
	"H_ID" NUMBER(4,0) NOT NULL ENABLE, 
	"CH_ID" NUMBER(3,0) NOT NULL ENABLE, 
	 CONSTRAINT "CITIZEN_HOUSE_PK" PRIMARY KEY ("H_ID", "PERSON_ID", "CH_ID");
   	 CONSTRAINT "CITIZEN_HOUSE_FK1" FOREIGN KEY ("PERSON_ID")
	  REFERENCES "FURRR"."CITIZEN" ("PERSON_ID") ENABLE, 
	 CONSTRAINT "CITIZEN_HOUSE_FK2" FOREIGN KEY ("H_ID")
	  REFERENCES "FURRR"."HOUSE" ("H_ID")
    
      CREATE TABLE "FURRR"."CITIZEN_HOUSE_CAR" 
   (	"CH_ID" NUMBER(3,0) NOT NULL ENABLE, 
	"N" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"CAR_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "CITIZEN_HOUSE_CAR_PK" PRIMARY KEY ("N", "CH_ID", "CAR_ID")
   	 CONSTRAINT "CITIZEN_HOUSE_CAR_FK1" FOREIGN KEY ("CAR_ID")
	  REFERENCES "FURRR"."CAR" ("CAR_ID")
    
    
    
   
   
